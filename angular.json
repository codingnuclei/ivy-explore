          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/renderComponentCE/tsconfig.app.json",
              "projects/renderComponentCE/tsconfig.spec.json",
              "projects/renderComponentCE/e2e/tsconfig.json"
            ],
            "exclude": [
