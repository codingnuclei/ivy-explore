            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/renderComponentCE/e2e/protractor.conf.js",
            "devServerTarget": "renderComponentCE:serve"
          },
          "configurations": {
